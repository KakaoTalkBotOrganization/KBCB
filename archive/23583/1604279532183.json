r song_image &#x3D; jsoup_result.select(&#x27;img&#x27;).attr(&#x27;data-original-src&#x27;).split(&quot;http&quot;)[2];\n        song_image &#x3D; decodeURIComponent(&#x27;http&#x27; + song_image);\n        var song_id &#x3D; jsoup_result.select(&#x27;a&#x27;).attr(&#x27;data-song-id&#x27;);\n        Kakao.send(room, {\n            link_ver: &quot;4.0&quot;,\n            template_id: 17141,\n            template_args: {\n                KMA: adult,\n                SONG_ID: song_id,\n                THUMB_URL: song_image,\n                TITLE: title,\n                ARTIST: artist\n            }\n        }, &quot;custom&quot;);\n    } catch (e) {\n        Api.replyRoom(room, &quot;정보를 찾지 못했습니다.&quot;);\n    }\n}</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <script type=\"text/data\" class=\"__se_module_data\" data-module='{\"type\":\"v2_code\", \"id\" : \"SE-2d5cb136-3b2c-4cb7-902b-64bf242aa4a0\"}'></script>\n                </div>                <div class=\"se-component se-text se-l-default\" id=\"SE-98f9a0ad-f139-4652-8f9b-68791d20661f\">\n                    <div class=\"se-component-content\">\n                        <div class=\"se-section se-section-text se-l-default\">\n                            <div class=\"se-module se-module-text\">\n\n                                    <!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-9988cf2e-a2a2-4245-9c56-ffd86db8ae8d\"><span style=\"color:#000000;\" class=\"se-fs- se-ff-   \" id=\"SE-ad55e9b9-9cd7-41e2-8e05-fecb59a7c600\">​</span></p><!-- } SE-TEXT -->\n                            </div>\n                        </div>\n                    </div>\n                </div>    </div>\n</div>",
