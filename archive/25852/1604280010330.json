            "contentHtml": "<div class=\"se-viewer se-theme-default\" lang=\"ko-KR\">\n    <!-- SE_DOC_HEADER_START -->\n    <!--@CONTENTS_HEADER-->\n    <!-- SE_DOC_HEADER_END -->\n    <div class=\"se-main-container\">\n                <div class=\"se-component se-code se-l-default\" id=\"SE-81a7b5d8-fdcf-45fc-b59b-5b52be632c95\">\n                    <div class=\"se-component-content\">\n                        <div class=\"se-section se-section-code se-l-default\">\n                            <div class=\"se-module se-module-code se-fs-fs13\">\n                                <div class=\"se-code-source\">\n                                    <div class=\"__se_code_view language-javascript\">const papagoTranslate &#x3D; require(&#x27;Papago&#x27;).papagoTranslate;\n\nconst lang &#x3D; {\n  &#x27;한국어&#x27;: &#x27;ko&#x27;,\n  &#x27;영어&#x27;: &#x27;en&#x27;,\n  &#x27;일본어&#x27;: &#x27;ja&#x27;,\n  &#x27;중국어(간체)&#x27;: &#x27;zh-CN&#x27;,\n  &#x27;중국어(번체)&#x27;: &#x27;zh-TW&#x27;,\n  &#x27;스페인어&#x27;: &#x27;es&#x27;,\n  &#x27;프랑스어&#x27;: &#x27;fr&#x27;,\n  &#x27;독일어&#x27;: &#x27;de&#x27;,\n  &#x27;러시아어&#x27;: &#x27;ru&#x27;,\n  &#x27;포르투갈어&#x27;: &#x27;pt&#x27;,\n  &#x27;이탈리아어&#x27;: &#x27;it&#x27;,\n  &#x27;베트남어&#x27;: &#x27;vi&#x27;,\n  &#x27;태국어&#x27;: &#x27;th&#x27;,\n  &#x27;인도네시아어&#x27;: &#x27;id&#x27;,\n  &#x27;힌디어&#x27;: &#x27;hi&#x27;\n};\n\nfunction response(room, msg, sender, igc, replier) {\n  let cmd &#x3D; msg.split(&#x27; &#x27;);\n  let test &#x3D; /[가-힣]/.test(msg);\n  if (cmd[0] &#x3D;&#x3D; &#x27;/번역&#x27;) {\n    const source &#x3D; test ? lang[cmd[1]] : cmd[1];\n    const target &#x3D; test ? lang[cmd[2]] : cmd[2];\n    const text &#x3D; cmd.slice(3).join(&#x27; &#x27;);\n    try {\n      replier.reply(papagoTranslate(source, target, text));\n    } catch (e) {\n      replier.reply(e);\n    }\n  }\n}</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n      