                    "content": "williameom const kalingModule = require('kaling').Kakao();\r\nconst Kakao = new kalingModule;\r\nKakao.init('key');\r\nKakao.login('id', 'password);\r\n\r\nfunction response(room, msg, sender, isGroupChat, replier, imageDB) {\r\n//날씨\r\nif (msg.indexOf(\"/날씨 \")==0) {\r\ntry {\r\nvar nss = org.jsoup.Jsoup.connect(\"https://search.daum.net/search?nil_suggest=btn&w=tot&DA=SBC&q=\"+encodeURI(msg.substr(4))+\" 날씨\").get();\r\nvar 온도 = nss.select(\"p.txt_desc\").text();\r\nvar 현재온도 = 온도.split(\"체감온도 \")[1].split(\"℃\")[0];\r\nvar 체감온도 = 온도.split(\"어제보다 \")[1].split(\"°C \")[0];\r\nvar 상태 = nss.select(\"p.txt_desc\").text().split(\" 체감온도\")[0];\r\nvar 날씨 = nss.select(\"span.txt_weather\").get(0).text().split(\", \")[1];\r\nvar 미세먼지 = nss.select(\"dd\").get(2).text();\r\nvar 습도 = nss.select(\"dd\").get(1).text();\r\nKakao.send(room, {\r\n\"link_ver\":\"4.0\",\r\n\"template_id\":(본인의 번호),\r\n\"template_args\":{\r\nheader : msg.substr(5) + \"의 날씨정보\",\r\nimg1 : \" \",\r\ntitle1 : \"날씨: \" + 날씨,\r\ndescription1 : 상태,\r\nimg2 : \" \",\r\ntitle2 : \"현재온도: \" + 현재온도 + \"°C\",\r\ndescription2 : \"체감온도: \" + 체감온도 + \"°C\",\r\nimg3 : \" \",\r\ntitle3 : \"미세먼지: \" + 미세먼지,\r\ndescription3 : \"습도: \" + 습도\r\n}\r\n}, \"custom\");\r\n} catch(e) {\r\nreplier.reply(msg.substr(4) + \"의 날씨정보가 없습니다.\"+e);\r\n}}\r\n\r\n",
