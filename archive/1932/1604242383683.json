                {
                    "content": "마지막에 +0.5가 성공 또는 실패 라는 답을 하는 확률이죠?",
                    "id": 58163247,
                    "isArticleWriter": true,
                    "isDeleted": false,
                    "isNew": false,
                    "isRef": true,
                    "isRemovable": false,
                    "refId": 58162969,
                    "updateDate": 1556243088000,
                    "writer": {
