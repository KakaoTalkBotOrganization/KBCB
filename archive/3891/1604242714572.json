            "contentHtml": "<p>function response(room, msg, sender, isGroupChat, replier) {<br /><br />wordpath = &quot;sdcard/katalkbot/Database/autoreply/data.txt&quot;<br /><br />roompath = &quot;sdcard/katalkbot/Database/autoreply/room.txt&quot;<br /><br />FS = FileStream.read(wordpath)<br /><br />RS = FileStream.read(roompath)<br /><br />if (msg==&quot;학습 켜기&quot;) {<br /><br />FileStream.append(roompath, room+&quot;\\n&quot;)<br /><br />replier.reply(&quot;자동학습이 &apos;&quot;+room+&quot;&apos; 방에 적용되었습니다.&quot;)<br /><br />}<br /><br />if (RS.split(&quot;\\n&quot;).indexOf(room)!=-1) {<br /><br />if(/^[^A-Za-z]*$/.test(msg)==true&amp;&amp;msg.length&gt;2) {<br /><br />var bl = [&quot;이발&quot;, &quot;봇&quot;, &quot;사진&quot;, &quot;이모티콘&quot;, &quot;동영상&quot;]<br /><br />for (var n = 0; n &lt; bl.length; n++) {<br /><br />if(msg.indexOf(bl[n])==-1){<br /><br />}<br /><br />}<br /><br />FileStream.append(wordpath,&quot;\\n&quot;+msg.split(&quot;\\n&quot;)[0])<br /><br />}<br /><br />r = Math.round(Math.random()*4)<br /><br />if (msg&amp;&amp;r==0) {<br /><br />num=FS.match(/\\n/g).length+1<br /><br />ran=Math.round(Math.random()*(num-1))<br /><br />answ=FS.split(&quot;\\n&quot;)[ran]<br /><br />replier.reply(answ)<br /><br />}<br /><br />if (msg==&quot;단어목록&quot;) {<br /><br />num=FS.match(/\\n/g).length+1<br /><br />replier.reply(&quot;현재 학습된 단어 목록\\n&quot;+num+&quot; 가지&quot;+&quot;\\u200b&quot;.repeat(500)+&quot;\\n\\n&quot;+FS)<br /><br />}<br /><br />if (msg==&quot;방목록&quot;) {<br /><br />num2=RS.match(/\\n/g).length+1<br /><br />replier.reply(&quot;현재 학습중인 방 목록\\n&quot;+num2+&quot; 가지&quot;+&quot;\\u200b&quot;.repeat(500)+&quot;\\n\\n&quot;+RS)<br /><br />}<br /><br />}<br /><br />}<br /><br /><br /><br />*/<br />도미의 추천글<br /><br /><a href=\"https://cafe.naver.com/nameyee/3880\" target=\"_blank\">https://cafe.naver.com/nameyee/3880</a><br />관리자 신청/수락<br /><br /><a href=\"https://cafe.naver.com/nameyee/3891\" target=\"_blank\">https://cafe.naver.com/nameyee/3