bsp;&nbsp;&nbsp;&nbsp; <span style=\"color:#A5C25C;\">&quot;당신의 철:&quot;</span> += 철;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <span style=\"color:#A5C25C;\">&quot;당신의골드:&quot;</span> += 골드;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <span style=\"color:#A5C25C;\">&quot;당신의 에메랄드:&quot;</span> += 에메랄드;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <span style=\"color:#A5C25C;\">&quot;당신의 다이아:&quot;</span> += 다이아;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 주택:&quot; += 주택;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;아파트:&quot; += 아파트;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 지역:&quot; += 지역;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 행성:&quot; += 행성;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 우주:&quot; += 우주;<br />\r\n&nbsp;&nbsp;&nbsp; }<br />\r\n&nbsp; } else {<br />\r\n&nbsp;&nbsp;&nbsp; replier.reply(&quot;재료(이)가 부족합니다!&quot;);<br />\r\n&nbsp; }<br />\r\n&nbsp; if (msg == &quot;#아파트&quot;) {<br />\r\n&nbsp;&nbsp;&nbsp; replier.reply(&quot;아파트를 사실려면 철 1000개하고 골드 50개가 필요합니다&quot;);<br />\r\n&nbsp;&nbsp;&nbsp; if (철 &gt;= 1000 &amp;&amp; 골드 &gt;= 50) {<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 철 -= 1000 &amp;&amp; 골드 -= 50 &amp;&amp; 아파트 += 1;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; replier.reply(&quot;아파트를 사셨습니다!당신의돈:&quot; + 코인);<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 철:&quot; += 철;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의골드:&quot; += 골드;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 에메랄드:&quot; += 에메랄드;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 다이아:&quot; += 다이아;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 주택:&quot; += 주택;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;아파트:&quot; += 아파트;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 지역:&quot; += 지역;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 행성:&quot; += 행성;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 우주:&quot; += 우주;<br />\r\n&nbsp;&nbsp;&nbsp; }<br />\r\n&nbsp; } else {<br />\r\n&nbsp;&nbsp;&nbsp; replier.reply(&quot;재료(이)가 부족합니다&quot;);<br />\r\n&nbsp; }<br />\r\n&nbsp; if (msg == &quot;#지역&quot;) {<br />\r\n&nbsp;&nbsp;&nbsp; replier.reply(&quot;지역을 살려면 철 50000개가 필요하고 에메랄드 100개와 다이아 50 필요니다&quot;);<br />\r\n&nbsp;&nbsp;&nbsp; if (철 &gt;= 50000 &amp;&amp; 에메랄드 &gt;= 100 &amp;&amp; 다이아 &gt;= 50) <br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 철 -= 500 &amp;&amp; 에메랄드 -= 3 &amp;&amp; 주택 += 1;<br />\r\n&nbsp;&nbsp;&nbsp; replier.reply(&quot;성공적으로 사셨습니다!당신의돈:&quot; + 코인);<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의 철:&quot; += 철;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의골드:&quot; += 골드;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의 에메랄드:&quot; += 에메랄드;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의 다이아:&quot; += 다이아;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의 주택:&quot; += 주택;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;아파트:&quot; += 아파트;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의 지역:&quot; += 지역;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의 행성:&quot; += 행성;<br />\r\n&nbsp;&nbsp;&nbsp; &quot;당신의 우주:&quot; += 우주;<br />\r\n&nbsp; } else {<br />\r\n&nbsp;&nbsp;&nbsp; replier.reply(&quot;재료(이)가 부족합니다!&quot;);<br />\r\n&nbsp; }<br />\r\n&nbsp; if (msg == &quot;#행성&quot;) {<br />\r\n&nbsp;&nbsp;&nbsp; replier.reply(&quot;행성을 살려면 철 100개가 필요합니다&quot;);<br />\r\n&nbsp;&nbsp;&nbsp; if (철 === 100) {<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 철 -= 100 &amp;&amp; 주택 += 1;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; replier.reply(&quot;성공적으로 사셨습니다!당신의돈:&quot; + 코인);<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 철:&quot; += 철;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의골드:&quot; += 골드;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 에메랄드:&quot; += 에메랄드;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 다이아:&quot; += 다이아;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 주택:&quot; += 주택;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;아파트:&quot; += 아파트;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 지역:&quot; += 지역;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 행성:&quot; += 행성;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &quot;당신의 우주:&quot; += 우주;<br />\r\n&nbsp;&nbsp;&nbsp