            "contentHtml": "<p>const scriptName=&quot;라이브러리.js&quot;;<br />FS=FileStream;<br />const nn=&quot;\\u200b&quot;.repeat(500);<br />function response(room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId){<br />msg=msg.trim();<br />msgn=msg.split(&quot; &quot;);<br />command=CommandParser.parse(msg,true);<br />try{<br />if(msg==&quot;.도서관&quot;){<br />replier.reply(&quot;도서관 사용법\\n책 목록 : .도서관 목록\\n책 추가 : .도서관 추가 (이름) (내용)\\n책 읽기 : .도서관 읽기 (ID)\\n\\n(도서 삭제는 MatSoGeum에게 말해주세요)\\n(이름이나 내용에 띄어쓰기가 있으면 \\&quot;로 감싸주세요.)&quot;);<br />}else if(msgn[0]==&quot;.도서관&quot;&amp;&amp;msgn[1]==&quot;추가&quot;){<br />Book=JSON.parse(FileStream.read(&quot;/sdcard/BOT/library/Book.json&quot;));<br />Book.push({name:command[2],content:command[3],writer:sender});<br />FileStream.write(&quot;/sdcard/BOT/library/Book.json&quot;,JSON.stringify(Book));<br />replier.reply(&quot;추가되었습니다.&quot;)<br />}else if(msgn[0]==&quot;.도서관&quot;&amp;&amp;msgn[1]==&quot;목록&quot;){<br />Book=JSON.parse(FileStream.read(&quot;/sdcard/BOT/library/Book.json&quot;));<br />Tmp=&quot;&quot;;<br />for(i=0;i&lt;Book.length;i++){<br />Tmp+=i<br />Tmp+=&quot; : &quot;;<br />Tmp+=Book[i][&apos;name&apos;];<br />Tmp+=&quot; &quot;;<br />Tmp+=Book[i][&apos;writer&apos;];<br />Tmp+=&quot;\\n&quot;;<br />}<br />replier.reply(Tmp);<br />}else if(msgn[0]==&quot;.도서관&quot;&amp;&amp;msgn[1]==&quot;읽기&quot;&amp;&amp;msgn[2]!=undefined){<br />Book=JSON.parse(FileStream.read(&quot;/sdcard/BOT/library/Book.json&quot;));<br />replier.reply(&quot;책&quot;+nn+&quot;\\n&quot;+Book[Number(msgn[2])][&apos;name&apos;]+&quot;\\n\\n&quot;+Book[Number(msgn[2])][&apos;content&apos;]);<br />}<br />}catch(e){<br />replier.reply(&quot;오류 발생&quot;);<br />}<br />}<br />//커맨드파서<br />var CommandParser = (function() {<br />    var parse = function(str, lookForQuotes) {<br />        var args = [];<br />        var readingPart = false;<br />        var part = &apos;&apos;;<br />        for (var i = 0; i &lt; str.length; i++){<br />            if (str.charAt(i) === &apos; &apos; &amp;&amp; !readingPart) {<br />                args.push(part);<br />                part = &apos;&apos;;<br />            } else {<br />                if (str.charAt(i) === &apos;\\&quot;&apos; &amp;&amp; lookForQuotes) {<br />                    readingPart = !readingPart;<br />                } else {<br />                    part += str.charAt(i);<br />                }<br />            }<br />        }<br />        args.push(part);<br />        return args;<br />    }<br />    return {<br />        parse: parse<br />    }<br />})();</p>",
