         break;\n                case &quot;잔고&quot;:\n                    Stock.isUE(s) ? r.reply(Stock.myInfo(s)) : r.reply(&quot;가입 먼저 해주세요.&quot;);\n                    break;\n                case &quot;종목&quot;:\n                    r.reply(Stock.cList());\n                    break;\n                case &quot;순위&quot;:\n                    r.reply(Stock.rank());\n            }\n        }\n        let t &#x3D; (new Date).getMinutes();\n        void 0 &#x3D;&#x3D;&#x3D; cStockV ? cStockV &#x3D; t : cStockV !&#x3D;&#x3D; t &amp;&amp; (cStockV &#x3D; t, Stock.cStockPrice());\n    } catch (t) {\n        r.reply(t);\n    }\n}\n\nfunction stockGame() {\n    this.list &#x3D; {\n        &quot;company A&quot;: {\n            price: 8900,\n            lnp: 8900,\n            cSv: 450\n        },\n        &quot;company B&quot;: {\n            price: 1500,\n            lnp: 1500,\n            cSv: 75\n        },\n        &quot;company C&quot;: {\n            price: 650,\n            lnp: 650,\n            cSv: 10\n        }\n    }, this.user &#x3D; {};\n}\nstockGame.prototype.join &#x3D; function(t) {\n    let e;\n    return void 0 &#x3D;&#x3D;&#x3D; this.user[t] ? (this.user[t] &#x3D; {}, this.user[t].wallet &#x3D; 100000, this.user[t].stock &#x3D; {}, Object.keys(this.list).map(e &#x3D;&gt; this.user[t].stock[e] &#x3D; 0), e &#x3D; &quot;가입 완료!&quot;) : e &#x3D; &quot;이미 가입했습니다.&quot;, e;\n}, stockGame.prototype.isUE &#x3D; function(t) {\n    return void 0 !&#x3D;&#x3D; this.user[t];\n}, stockGame.prototype.cList &#x3D; function() {\n    let t &#x3D; [];\n    for (let e in this.list) t.push(e + &quot;\\n-&gt; &quot; + this.nC(this.list[e].price) + &quot;₩&quot;);\n    return t.join(&quot;\\n\\n&quot;);\n}, stockGame.prototype.type &#x3D; function(t, e, s) {\n    if (!this.nValue(s.item)) throw new Error(&quot;Item Does Not Exist&quot;);\n    if (this.cN(s.amount)) {\n        let i;\n        switch (t) {\n            case &quot;sell&quot;:\n                i &#x3D; this.sell(e, s.item, s.amount);\n                break;\n            case &quot;buy&quot;:\n                i &#x3D; this.buy(e, s.item, s.amount);\n                break;\n            default:\n                throw new Error(&quot;Type Not Allowed&quot;);\n        }\n        return i;\n    }\n}, stockGame.prototype.nValue &#x3D; function(t) {\n    return void 0 !&#x3D;&#x3D; this.list[t];\n}, stockGame.prototype.nC &#x3D; function(t) {\n    return t.toString().replace(/\\B(?&#x3D;(\\d{3})+(?!\\d))/g, &quot;,&quot;);\n}, stockGame.prototype.myInfo &#x3D; function(t) {\n    let e, s &#x3D; [],\n        i &#x3D; this.user[t].wallet;\n    if (void 0 !&#x3D;&#x3D; this.user[t]) {\n        for (let e in this.user[t].stock) s.push(e + &quot; | &quot; + this.user[t].stock[e] + &quot;주&quot;);\n        for (let e in this.user[t].stock) i +&#x3D; this.list[e].price * this.user[t].stock[e];\n        e &#x3D; &quot;[내 주식 잔고]\\n닉네임: &quot; + t + &quot;\\n보유 자산: &quot; + this.nC(this.user[t].wallet) + &quot;₩\\n총평가금액: &quot; + this.nC(i) + &quot;₩\\n\\n-보유 주식-\\n&quot; + s.join(&quot;\\n&quot;);\n    } else e &#x3D; &quot;유저 정보를 찾을 수 없습니다.&quot;;\n    return e;\n}, stockGame.prototype.sell &#x3D; function(t, e, s) {\n    if (this.cN(s)) {\n        let i &#x3D; this.list[e].price;\n        return this.user[t].stock[e] &gt;&#x3D; Number(s) ? (this.user[t].wallet +&#x3D; i * Number(s), this.user[t].stock[e] -&#x3D; Number(s), &quot;[주문 체결]\\n\\n종목: &quot; + e + &quot;\\n매도 가격: &quot; + this.nC(i) + &quot;₩\\n수량: &quot; + s + &quot;\\n\\n종합: +&quot; + this.nC(i * Number(s)) + &quot;₩&quot;) : &quot;매도 가능 물량을 초과했습니다&quot;;\n    }\n}, stockGame.prototype.cN &#x3D; function(t) {\n    if (/^(\\-|\\+)?([0-9]+)$/.test(t) &amp;&amp; parseInt(t) &gt; 0) return !0;\n    throw new Error(&quot;Number Must Be Integer&quot;);\n}, stockGame.prototype.buy &#x3D; function(t, e, s) {\n    if (this.cN(s)) {\n        let i &#x3D; this.list[e].price;\n        return this.user[t].wallet &gt;&#x3D; i * Number(s) ? (this.user[t].wallet -&#x3D; i * Number(s), this.user[t].stock[e] +&#x3D; Number(s), &quot;[주문 체결]\\n\\n종목: &quot; + e + &quot;\\n매수 가격: &quot; + this.nC(i) + &quot;₩\\n수량: &quot; + s + &quot;\\n\\n종합: -&quot; + this.nC(i * Number(s)) + &quot;₩&quot;) : &quot;주문 가능 가격을 초과했습니다.&quot;;\n    }\n}, stockGame.prototype.rN &#x3D; function(t) {\n    let e &#x3D; Math.random(),\n        s &#x3D; Math.floor(e * t + 1);\n    return e &gt; .5 ? s &#x3D; s : s *&#x3D; -1, s;\n}, stockGame.prototype.rank &#x3D; function() {\n    let t &#x3D; [];\n    for (let e in this.user) t.push({\n        nickname: e,\n        wallet: this.user[e].wallet\n    });\n    t.sort((t, e) &#x3D;&gt; e.wallet - t.wallet);\n    for (let e in t) t[e] &#x3D; Number(e) + 1 + &quot;위 | &quot; + t[e].nickname + &quot;\\n보유 자산: &quot; + this.nC(t[e].wallet) + &quot;₩&quot;;\n    return t.join(&quot;\\n\\n&quot;);\n}, stockGame.prototype.cStockPrice &#x3D; function() {\n    for (let t in this.list) this.list[t].price +&#x3D; this.rN(this.list[t].cSv);\n};</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <script type=\"text/data\" class=\"__se_module_data\" data-module='{\"type\":\"v2_code\", \"id\" : \"SE-440a6524-8836-4098-953d-793dcbcbf9a7\"}'></script>\n                </div>                <div class=\"se-component se-text se-l-default\" id=\"SE-2eb200cb-7923-4777-99e2-7e66b0d574c3\">\n                    <div class=\"se-component-content\">\n                        <div class=\"se-section se-section-text se-l-default\">\n                            <div class=\"se-module se-module-text\">\n\n                                    <!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-FC442B49-70F7-4A57-84AE-B90808473C3C\"><span style=\"\" class=\"se-fs- se-ff-   \" id=\"SE-A6D7F823-2F42-4093-9B49-933F8F059972\">​</span></p><!-- } SE-TEXT -->\n    