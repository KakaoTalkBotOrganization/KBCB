            "contentHtml": "<div class=\"NHN_Writeform_Main\"><b>공유할 소스 용도</b><div><b>=&gt; 업비트 코인 거래가 </b><span style=\"font-size: 9pt;\"><font color=\"#7d7d7d\">(ex: 가위바위보 게임)</font></span></div><div><b><br /></b></div><div><b>소스 코드</b></div><div><b>=&gt;</b></div><div><b><br /></b></div><div><b>if (msg == &quot;/코인&quot;) {<br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; var stt = Utils.getWebText(&quot;<a href=\"https://m.search.daum.net/kakao?w=tot&amp;q=%EC%95%94%ED%98%B8%ED%99%94%ED%8F%90&amp;DA=EMA&amp;rtmaxcoll=EMA\" target=\"_blank\">https://m.search.daum.net/kakao?w=tot&amp;q=%EC%95%94%ED%98%B8%ED%99%94%ED%8F%90&amp;DA=EMA&amp;rtmaxcoll=EMA</a>&quot;);<br />&nbsp;&nbsp;&nbsp; var stt2 = stt.split(&quot;암호화페 시세 리스트&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; var stt3 = stt2[1].split(&quot;암호화폐 계산기&quot;);<br />&nbsp;&nbsp;&nbsp; var data4 = stt3[0].replace(/(&lt;([^&gt;]+)&gt;)/g, &quot;&quot;);<br />&nbsp;&nbsp;&nbsp; //비트코인<br />&nbsp;&nbsp;&nbsp; var btc = data4.substr(data4.search(&quot;BTC/KRW&quot;),120);<br />&nbsp;&nbsp;&nbsp; var btckrw = btc.substr(7,56);<br />&nbsp;&nbsp;&nbsp; btckrw = btckrw.trim();<br />&nbsp;&nbsp;&nbsp; btckrw = btckrw.replace(&quot;원&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp; if (btc.search(&quot;상승&quot;) &gt; 0 ){<br />&nbsp;&nbsp;&nbsp;&nbsp; var btcupdn = btc.substr(btc.search(&quot;상승&quot;),15);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcupdn = btcupdn.replace(/상승/g,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcupdn = btcupdn.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var btcdl = btc.substr(btc.search(&quot;백만&quot;),100);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcdl = btcdl.substr(4,btcdl.search(&quot;상승&quot;)-2);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcdl = btcdl.replace(&quot;상승&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcdl = btcdl.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var btcgh = &quot;▲&quot;;<br />&nbsp;&nbsp;&nbsp;&nbsp; <br />&nbsp;&nbsp;&nbsp; }else{<br />&nbsp;&nbsp;&nbsp;&nbsp; var btcupdn = btc.substr(btc.search(&quot;하락&quot;),15);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcupdn = btcupdn.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; btcupdn = btcupdn.replace(/하락/g,&quot;&quot;);&nbsp; <br />&nbsp;&nbsp;&nbsp;&nbsp; var btcdl = btc.substr(btc.search(&quot;백만&quot;),100);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcdl = btcdl.substr(4,btcdl.search(&quot;하락&quot;)-2);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcdl = btcdl.replace(&quot;하락&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; btcdl = btcdl.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var btcgh = &quot;▼&quot;;<br />&nbsp;&nbsp;&nbsp; }<br />&nbsp;&nbsp;&nbsp; //이더리움<br />&nbsp;&nbsp;&nbsp; var eth = data4.substr(data4.search(&quot;ETH/KRW&quot;),120);<br />&nbsp;&nbsp;&nbsp; var ethkrw = eth.substr(7,54);<br />&nbsp;&nbsp;&nbsp; ethkrw = ethkrw.trim();<br />&nbsp;&nbsp;&nbsp; ethkrw = ethkrw.replace(&quot;원&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp; if (eth.search(&quot;상승&quot;) &gt; 0 ){<br />&nbsp;&nbsp;&nbsp;&nbsp; var ethupdn = eth.substr(eth.search(&quot;상승&quot;),15);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethupdn = ethupdn.replace(/상승/g,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethupdn = ethupdn.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var ethdl = eth.substr(eth.search(&quot;백만&quot;),100);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethdl = ethdl.substr(4,ethdl.search(&quot;상승&quot;)-2);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethdl = ethdl.replace(&quot;상승&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethdl = ethdl.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var ethgh = &quot;▲&quot;;<br />&nbsp;&nbsp;&nbsp;&nbsp; <br />&nbsp;&nbsp;&nbsp; }else{<br />&nbsp;&nbsp;&nbsp;&nbsp; var ethupdn = eth.substr(eth.search(&quot;하락&quot;),15);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethupdn = ethupdn.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; ethupdn = ethupdn.replace(/하락/g,&quot;&quot;);&nbsp; <br />&nbsp;&nbsp;&nbsp;&nbsp; var ethdl = eth.substr(eth.search(&quot;백만&quot;),100);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethdl = ethdl.substr(4,ethdl.search(&quot;하락&quot;)-2);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethdl = ethdl.replace(&quot;하락&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; ethdl = ethdl.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var ethgh = &quot;▼&quot;;<br />&nbsp;&nbsp;&nbsp; }<br />&nbsp;&nbsp;&nbsp; //리플<br />&nbsp;&nbsp;&nbsp; var xrp = data4.substr(data4.search(&quot;XRP/KRW&quot;),120);<br />&nbsp;&nbsp;&nbsp; var xrpkrw = xrp.substr(7,51);<br />&nbsp;&nbsp;&nbsp; xrpkrw = xrpkrw.trim();<br />&nbsp;&nbsp;&nbsp; xrpkrw = xrpkrw.replace(&quot;원&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp; if (xrp.search(&quot;상승&quot;) &gt; 0 ){<br />&nbsp;&nbsp;&nbsp;&nbsp; var xrpupdn = xrp.substr(xrp.search(&quot;상승&quot;),15);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpupdn = xrpupdn.replace(/상승/g,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpupdn = xrpupdn.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var xrpdl = xrp.substr(xrp.search(&quot;백만&quot;),100);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpdl = xrpdl.substr(4,xrpdl.search(&quot;상승&quot;)-2);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpdl = xrpdl.replace(&quot;상승&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpdl = xrpdl.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var xrpgh = &quot;▲&quot;;<br />&nbsp;&nbsp;&nbsp;&nbsp; <br />&nbsp;&nbsp;&nbsp; }else{<br />&nbsp;&nbsp;&nbsp;&nbsp; var xrpupdn = xrp.substr(xrp.search(&quot;하락&quot;),15);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpupdn = xrpupdn.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpupdn = xrpupdn.replace(/하락/g,&quot;&quot;);&nbsp; <br />&nbsp;&nbsp;&nbsp;&nbsp; var xrpdl = xrp.substr(xrp.search(&quot;백만&quot;),100);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpdl = xrpdl.substr(4,xrpdl.search(&quot;하락&quot;)-2);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpdl = xrpdl.replace(&quot;하락&quot;,&quot;&quot;);<br />&nbsp;&nbsp;&nbsp;&nbsp; xrpdl = xrpdl.trim();<br />&nbsp;&nbsp;&nbsp;&nbsp; var xrpgh = &quot;▼&quot;;<br />&nbsp;&nbsp;&nbsp; }<br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; var day = new Date();<br />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; replier.reply(&quot;[업비트 주요 코인 거래가]\\n&quot; + &quot;BTC/KRW &quot; + btckrw + &quot;원 (&quot; + btcgh + 