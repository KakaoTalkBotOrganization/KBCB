            "contentHtml": "[[[CONTENT-ELEMENT-0]]]<br /><p>대충 이리 뜬다<br /><br /></p><p>const scriptName = &quot;호출&quot;;</p><p>importPackage(android.widget);</p><p>var ctx = Api.getContext();</p><p><br /></p><p>function response(room, msg, sender, isGroupChat, replier, imageDB, packageName) {</p><p>&nbsp; &nbsp; if (msg == &quot;호출&quot;) {</p><p>&nbsp; &nbsp; &nbsp; &nbsp; Api.UIThread(function() {</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; var lay = new LinearLayout(ctx);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; lay.setOrientation(1);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; lay.setBackgroundColor(-1);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; var img = new ImageView(ctx);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; img.setImageBitmap(imageDB.getProfileBitmap());</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; lay.addView(img);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; var r = new TextView(ctx);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; r.setText(&quot;Room : &quot; + room);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; lay.addView(r);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; var s = new TextView(ctx);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; s.setText(&quot;Sender : &quot; + sender);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; lay.addView(s);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; var toast = new Toast(ctx);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toast.setGravity(17, 0, 0);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toast.setView(lay);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; toast.show();</p><p>&nbsp; &nbsp; &nbsp; &nbsp; });</p><p>&nbsp; &nbsp; }</p><p>}</p><p><br />(기준에 따르면 고급 소스던데)</p>",
