                    "content": "감사합니다 :)",
                    "id": 98169361,
                    "isArticleWriter": true,
                    "isDeleted": false,
                    "isNew": false,
                    "isRef": true,
                    "isRemovable": false,
                    "refId": 98168922,
                    "updateDate": 1595919818000,
                    "writer": {
