            "contentHtml": "<p>&nbsp;</p><p>var useroom=&quot;사용할방 이름&quot;</p><p>&nbsp;</p><p>var yesterday=0</p><p>&nbsp;</p><p>var list=[]</p><p>&nbsp;</p><p>var rank=1</p><p>&nbsp;</p><p>var lists=&quot;&quot;</p><p>&nbsp;</p><p>&nbsp;</p><p>function response(room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId){</p><p>&nbsp;</p><p>&nbsp;</p><p>var dd=new Date();</p><p>&nbsp;</p><p>var today=dd.getDate()</p><p>&nbsp;</p><p>if(yesterday != today){</p><p>&nbsp;</p><p>yesterday=today</p><p>&nbsp;</p><p>list=[]</p><p>&nbsp;</p><p>rank=1</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>if(room==useroom){</p><p>&nbsp;</p><p>if(msg==&quot;출석&quot;){</p><p>&nbsp;</p><p>if(list.indexOf(sender)!=-1){</p><p>&nbsp;</p><p>replier.reply(&quot;이미 출석하셨습니다&quot;)</p><p>&nbsp;</p><p>}else{</p><p>&nbsp;</p><p>list.push(sender);</p><p>&nbsp;</p><p>&nbsp;replier.reply(sender+ &quot;님&quot;+rank+&quot;등으로 출석하셨습니다&quot;);</p><p>&nbsp;</p><p>rank=rank+1</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>&nbsp;</p><p>&nbsp;</p><p>&nbsp;</p><p>&nbsp;</p><p>if(msg.indexOf(&quot;출석목록&quot;)==0){</p><p>&nbsp;</p><p>if(list.length==0){</p><p>&nbsp;</p><p>replier.reply(&quot;아직 아무도 출석하지 않으셨습니다&quot;)</p><p>&nbsp;</p><p>}else if(list.length&gt;0){</p><p>&nbsp;</p><p>lists=&quot;&quot;</p><p>&nbsp;</p><p>for(j=0; j&lt;list.length; j++){</p><p>&nbsp;</p><p>var p=j+1</p><p>&nbsp;</p><p>lists +=&quot;[&quot;+p+&quot;]&quot;+list[j]+&quot;\\n&quot;</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>replier.reply(&quot;[출석부]\\n&quot;+room+&quot;\\u200b&quot;.repeat(500)+&quot;\\nㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡ\\n&quot;+ lists);</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>}</p><p>&nbsp;</p><p>&nbsp;</p>",
