                    "content": "const scriptName=\"인사봇.js\";\r\nconst Thread = java.lang.Thread;\r\nconst Jsoup = org.jsoup.Jsoup;\r\n\r\nlet room_raw = [\"https://develope.dev/_s/roomInfo?api=64daf1c33be2333d1574784388&room=%22%EC%B9%98%ED%82%A8%EB%B4%87%EB%B0%A9%22\"]; //사용할 방 주소 순서대로 추가하시면 됩니다.\r\nlet room_names = [\"치킨봇방\"]; //사용할 방 이름 정확하게 순서대로 추가하시면 됩니다.\r\nlet data_real = [];\r\nlet data_pre = [];\r\n\r\nlet loopStarted = false;\r\n\r\nfunction response(room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId) {\r\n   try{\r\n   if (!loopStarted) {\r\n      loopStarted = true;\r\n      \r\n      while (true) {\r\n         updateData();\r\n         for (let i = 0; i < room_raw.length; i ++) {\r\n            if (data_pre[i] != null) {\r\n               if (data_real[i].split(\",\")[0] > data_pre[i].split(\",\")[0]){\r\n                  Api.replyRoom(room_names[i], \"어서와요..\");\r\n               }\r\n               else if (data_real[i].split(\",\")[0] < data_pre[i].split(\",\")[0]){\r\n                  Api.replyRoom(room_names[i], \"잘가요...\");\r\n               }\r\n               if (data_real[i].split(\",\")[1] > data_pre[i].split(\",\")[1]){\r\n                  Api.replyRoom(room_names[i], \"누군가 하트를 눌렀어요!\");\r\n               }\r\n               else if (data_real[i].split(\",\")[1] < data_pre[i].split(\",\")[1]){\r\n                  Api.replyRoom(room_names[i], \"누군가 하트를 취소했어요..\");\r\n               }\r\n            }\r\n         }      \r\n         \r\n         Thread.sleep(1000);\r\n      }\r\n   }\r\n   }catch(e){\r\n      err = \"오류내용 : \"+e+\"\\n에러발생 행 : \"+e.lineNumber;\r\n              Api.replyRoom(\"man\", err);\r\n       }\r\n}\r\n\r\nupdateData = () => {\r\n   for (let i = 0; i < room_raw.length; i ++) {\r\n      data_pre[i] = data_real[i]; // copy previous data_real to data_pre\r\n      \r\n      let result = org.jsoup.Jsoup.connect(room_raw[i]).get().select(\"body\").text();\r\n   let people = result.split(\"인원수: \")[1].split(\"명\")[0];\r\n   let heart = result.split(\"하트: \")[1].split(\"개\")[0];\r\n      data_real[i] = people + \",\" + heart;\r\n   }\r\n}",
