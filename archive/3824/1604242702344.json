                    "content": "function distance(x1, y1, x2, y2) {\r\n    return Math.sqrt(Math.pow(x1 - x2, 2) + Math.pow(y1 - y2, 2));\r\n}\r\n\r\nfunction parse(text) {\r\n    const regex = /^\\/거리\\s+\\(([+-]?\\d+(?:\\.\\d+)?),\\s*([+-]?\\d+(?:\\.\\d+)?)\\)\\s+\\(([+-]?\\d+(?:\\.\\d+)?),\\s*([+-]?\\d+(?:\\.\\d+)?)\\)$/;\r\n    if (regex.test(text)) {\r\n        const matches = regex.exec(text);\r\n        return [Number(matches[1]), Number(matches[2]), Number(matches[3]), Number(matches[4])];\r\n    }\r\n    throw new Error(\"Cannot parse the input\");\r\n}\r\n\r\nconst input = \"/거리 (1,2) (3,4)\";\r\nconst args = parse(input);\r\nconsole.log(distance(args[0], args[1], args[2], args[3]));\r\n\r\n이렇게 하면 될듯 안되면 알려주세요",
