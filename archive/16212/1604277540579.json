br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <span style=\"color:#CB772F;\">if</span>(내정보[<span style=\"color:#6897BB;\">3</span>]&gt;<span style=\"color:#6897BB;\">0</span> &amp;&amp; three==<span style=\"color:#6897BB;\">0</span> &amp;&amp; msg == <span style=\"color:#A5C25C;\">&quot;&amp;3배 포인트&quot;</span>){<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 내정보[<span style=\"color:#6897BB;\">2</span>]=내정보[<span style=\"color:#6897BB;\">2</span>]*<span style=\"color:#6897BB;\">3</span>;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 내정보[<span style=\"color:#6897BB;\">3</span>]=내정보[<span style=\"color:#6897BB;\">3</span>]-<span style=\"color:#6897BB;\">1</span>;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; replier.reply(<span style=\"color:#FFFFFF;\">sender+</span><span style=\"color:#FFFFFF;\"><span style=\"color:#A5C25C;\">&quot;님이 성공 현제 포인트:&quot;</span></span><span style=\"color:#FFFFFF;\">+내정보[</span><span style=\"color:#FFFFFF;\"><span style=\"color:#6897BB;\">2</span></span><span style=\"color:#FFFFFF;\">]+</span><span style=\"color:#FFFFFF;\"><span style=\"color:#A5C25C;\">&quot;포인트&quot;</span></span>);<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; }<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <span style=\"color:#CB772F;\">if</span>(내정보[<span style=\"color:#6897BB;\">3</span>]&gt;<span style=\"color:#6897BB;\">0</span> &amp;&amp; three==<span style=\"color:#6897BB;\">1</span> &amp;&amp; msg == <span style=\"color:#A5C25C;\">&quot;&amp;3배 포인트&quot;</span>){<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 내정보[<span style=\"color:#6897BB;\">2</span>]=내정보[<span style=\"color:#6897BB;\">2</span>]/<span style=\"color:#6897BB;\">2</span>;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 내정보[<span style=\"color:#6897BB;\">3</span>]=내정보[<span style=\"color:#6897BB;\">3</span>]-<span style=\"color:#6897BB;\">1</span>;<br />\r\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; replier.reply(<span style=\"color:#FFFFFF;\">sender+ </span>