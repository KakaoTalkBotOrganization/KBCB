                    "content": "ㅓ라\n\n\nFS = FileStream;\npath = 'sdcard/list.txt';\nfunction response(room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId) {\n  if (FS.read(path) == null) {\n    FS.write(path, '{}')\n  }\n  rd = JSON.parse(FS.read(path))\n  if (msg == \"!투표시작\") {\n    if (rd[room] == undefined) {\n      rd[room] == {};\n      rd[room]['1'] = 0;\n      rd[room]['2'] = 0;\n      rd[room]['3'] = 0;\n      rd[room]['4'] = 0;\n      rd[room]['5'] = 0;\n      rd[room]['vote'] = false;\n    }\n    rd[room]['vote'] = true;\n    replier.reply(\"투표를 시작합니다. !1, !2, !3, !4, !5 중 하나를 작성해 투표하세요.\");\n  }\n  if (msg.startsWith(\"!\") && rd[room]['vote'] == true) {\n    msg = msg.substr(1);\n    if (msg == 1 || msg == 2 || msg == 3 || msg == 4 || msg == 5) {\n      rd[room][msg]++;\n    }\n  }\n  if (msg.startsWith(\"*\") && rd[room]['vote'] == true) {\n    msg = msg.substr(1);\n    if (msg == 1 || msg == 2 || msg == 3 || msg == 4 || msg == 5) {\n      replier.reply(rd[room][msg]);\n    }\n  }\n  if (msg == \"!결과\" && rd[room]['vote'] == true) {\n    replier.reply(\"1번: \"+ rd[room]['1'] + '회\\n2번: ' + rd[room]['2'] + '회\\n3번: ' + rd[room]['3'] + '회\\n4번: ' + rd[room]['4'] + '회\\n5번: ' + rd[room]['5'] + '회');\n  }\n  if (msg == \"!리셋\" || msg == \"!R\") {\n    rd[room]['vote'] == false;\n    rd[room]['1'] = 0;\n    rd[room]['2'] = 0;\n    rd[room]['3'] = 0;\n    rd[room]['4'] = 0;\n    rd[room]['5'] = 0;\n    replier.reply(\"리셋되었습니다!\");\n  }\n}",
