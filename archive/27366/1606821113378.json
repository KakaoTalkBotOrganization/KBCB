            "contentHtml": "<div class=\"se-viewer se-theme-default\" lang=\"ko-KR\">\n    <!-- SE_DOC_HEADER_START -->\n    <!--@CONTENTS_HEADER-->\n    <!-- SE_DOC_HEADER_END -->\n    <div class=\"se-main-container\">\n                <div class=\"se-component se-text se-l-default\" id=\"SE-fa3a1eab-23f3-11eb-965d-b1cf6961b8e1\">\n                    <div class=\"se-component-content\">\n                        <div class=\"se-section se-section-text se-l-default\">\n                            <div class=\"se-module se-module-text\">\n\n                                    <!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cef8928-23f4-11eb-965d-974d48e729f4\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cef8927-23f4-11eb-965d-6f66dc3a311b\">function (n, e, m) {</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cef892a-23f4-11eb-965d-97a8a526ea27\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cef8929-23f4-11eb-965d-d1731f9d4c81\">    n &#x3D; new java.math.BigInteger(n, 16);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cef892c-23f4-11eb-965d-677fb4a6900c\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cef892b-23f4-11eb-965d-9dbe80c4c192\">    e &#x3D; new java.math.BigInteger(e, 16);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cef892e-23f4-11eb-965d-9140d0b4addd\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cef892d-23f4-11eb-965d-f39b93410920\">    m &#x3D; new java.lang.String(m);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cef8930-23f4-11eb-965d-45411d83da38\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cef892f-23f4-11eb-965d-03f5fd685ab2\">    const KeyFactory &#x3D; java.security.KeyFactory.getInstance(&#x27;RSA&#x27;);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cef8932-23f4-11eb-965d-37ece385addc\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cef8931-23f4-11eb-965d-4550ca5bc0c3\">    const RSAPublicKeySpec &#x3D; new java.security.spec.RSAPublicKeySpec(n, e);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cef8934-23f4-11eb-965d-819c71ece94c\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cef8933-23f4-11eb-965d-959b79986151\">    const Key &#x3D; KeyFactory.generatePublic(RSAPublicKeySpec);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb046-23f4-11eb-965d-b50d4cfe51e9\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb045-23f4-11eb-965d-03794014e9f1\">    const cipher &#x3D; javax.crypto.Cipher.getInstance(&#x27;RSA/None/PKCS1Padding&#x27;);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb048-23f4-11eb-965d-87dcaee1734a\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb047-23f4-11eb-965d-f5b2b6946c16\">    cipher.init(javax.crypto.Cipher.ENCRYPT_MODE, Key);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb04a-23f4-11eb-965d-0f7a9ad4b4d1\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb049-23f4-11eb-965d-bf275b62d510\">    const bytes &#x3D; m.getBytes(&#x27;UTF-8&#x27;);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb04c-23f4-11eb-965d-a95ab67f377a\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb04b-23f4-11eb-965d-4d28c1aa4252\">    const byte &#x3D; cipher.doFinal(bytes);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb04e-23f4-11eb-965d-278b55347348\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb04d-23f4-11eb-965d-852b5c677b2a\">    const encoder &#x3D; java.util.Base64.getEncoder();</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb050-23f4-11eb-965d-c9b38a12e481\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb04f-23f4-11eb-965d-b578ad8507fb\">    const encoded &#x3D; encoder.encodeToString(byte);</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb052-23f4-11eb-965d-a51c1a3228bb\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb051-23f4-11eb-965d-1bc513650f43\">    return encoded.toString();</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb054-23f4-11eb-965d-4d5fe8597d1b\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb053-23f4-11eb-965d-5111bf2c4e41\">}</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefb056-23f4-11eb-965d-83422901465d\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefb055-23f4-11eb-965d-b341785592b4\">​</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefd768-23f4-11eb-965d-e92c94a787ee\"><span style=\"\" class=\"se-fs- se-ff-nanumsquare   \" id=\"SE-2cefd767-23f4-11eb-965d-35a9455d80ee\">​</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"\" id=\"SE-2cefd76a-23f4-11eb-965d-efb8d211c7c2\"><span style=\"\" class=\"se-fs- se-ff-