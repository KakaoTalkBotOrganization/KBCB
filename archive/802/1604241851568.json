            "contentHtml": "//간단하게 소스를 짜보면<div><br /></div><div>const chatLank = {};</div><div><br /></div><div><div style=\"font-size: 13.3333px;\">function respons (room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId) {</div></div><div><br /></div><div><div><span style=\"font-size: 10pt;\">if(sender in chatLank) chatLank[sender] ++;</span></div><div>else chatLank[sender] = 0;</div></div><div><br /></div><div>if(msg===&quot;채팅순위&quot;) {</div><div>var lanking = &quot;&quot;;</div><div>for(var key in chatLank){</div><div>lanking += &quot;\\n&quot;+key+&quot; : &quot;+chatLank[key];</div><div>}<br />replier.reply(&quot;채팅순위입니다&quot;+&quot;\\u200b&quot;.repeat(500)+lanking)</div><div>}</div><div><br /></div><div><span style=\"font-size: 10pt;\">}</span></div><div><span style=\"font-size: 10pt;\"><br /></span></div><div><span style=\"font-size: 10pt;\"><br /></span></div><div><span style=\"font-size: 10pt;\"><br /></span></div><div><span style=\"font-size: 10pt;\">//이걸 응용해서 순위를 sort하는 소스를 짜면</span></div><div><span style=\"font-size: 10pt;\"><br /></span></div><div><div style=\"\">function sortObject(obj) {</div><div style=\"\">&nbsp; return Object.keys(obj)</div><div style=\"\">&nbsp; &nbsp; .sort().reduce((a, v) =&gt; {</div><div style=\"\">&nbsp; &nbsp; a[v] = obj[v];</div><div style=\"\">&nbsp; &nbsp; return a; }, {});</div><div style=\"\">}</div></div><div style=\"\"><br /></div><div><span style=\"font-size: 13.3333px;\">const chatLank = {};</span></div><div><span style=\"font-size: 10pt;\"><br /></span></div><div><div style=\"\"><div style=\"font-size: 13.3333px;\">function respons (room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId) {</div><div style=\"font-size: 13.3333px;\"><br /></div><div style=\"font-size: 13.3333px;\"><div><span style=\"font-size: 10pt;\">if(sender in chatLank) chatLank[sender] ++;</span></div><div>else chatLank[sender] = 0;</div></div><div style=\"font-size: 13.3333px;\"><br /></div><div style=\"font-size: 13.3333px;\">if(msg===&quot;채팅순위&quot;) {</div><div style=\"font-size: 13.3333px;\">var lank = sortObject(chatLank);</div><div style=\"font-size: 13.3333px;\"><span style=\"font-size: 13.3333px;\">var lanking = &quot;&quot;;</span></div><div style=\"font-size: 13.3333px;\">for(var key in lank){</div><div style=\"font-size: 13.3333px;\">lanking += &quot;\\n&quot;+key+&quot; : &quot;+lank[key];</div><div style=\"font-size: 13.3333px;\">}<br />replier.reply(&quot;채팅순위입니다&quot;+&quot;\\u200b&quot;.repeat(500)+lanking)</div><div style=\"font-size: 13.3333px;\">}</div><div style=\"font-size: 13.3333px;\"><br /></div><div style=\"font-size: 13.3333px;\"><span style=\"font-size: 10pt;\">}</span></div></div></div><div><span style=\"font-size: 10pt;\"><br /></span></div>",
