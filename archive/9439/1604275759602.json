yle=\"\"><span style=\"font-size: 10pt;\">curl_setopt($ch, CURLOPT_SSL_VERIFYPEER, false);</span></div><div style=\"\"><span style=\"font-size: 10pt;\">$res = curl_exec($ch);</span></div><div style=\"\"><span style=\"font-size: 10pt;\">curl_close($ch);</span></div><div style=\"\"><span style=\"font-size: 10pt;\">print_r($res, true);</span></div><div style=\"\"><span style=\"font-size: 10pt;\">?&gt;</span></div></div><div style=\"\"><span style=\"font-size: 10pt;\"><br /></span></div><div style=\"\"><span style=\"font-size: 10pt;\">[[[CONTENT-ELEMENT-0]]]<br /></span></div></div>",
