            "contentHtml": "<p>var num=0;<br />var hum=[];<br />var a=null;<br />var st=false;<br />var b=null;<br /><br />function response(params) {<br />if(params.msg==&quot;/31 입장&quot;) {<br />if(hum.indexOf(params.sender) == -1) {<br />hum.push(params.sender);<br />params.replier.reply(&quot;&apos;&quot;+params.sender+&quot;&apos; 님이 게임에 참가했어요.\\n\\n[현재참가자]\\n\\n&quot;+hum.join(&quot;\\n&quot;));<br />} else {<br />params.replier.reply(&quot;&apos;&quot;+params.sender+&quot;&apos;님은 이미 참가되어 있습니다.\\n\\n[현재참가자]\\n\\n&quot;+hum.join(&quot;\\n&quot;));<br />}<br />}<br />if(params.msg==&quot;/31 퇴장&quot;) {<br />if(hum.indexOf(params.sender) != -1) {<br />hum.splice(hum.indexOf(params.sender),1);<br />params.replier.reply(&quot;&apos;&quot;+params.sender+&quot;&apos;님이 퇴장하였습니다.&quot;);<br />} else {<br />params.replier.reply(&quot;&apos;&quot;+params.sender+&quot;&apos;님은 참가되어 있지 않습니다.&quot;);<br />}<br />}<br />if(params.msg==&quot;/31 시작&quot; &amp;&amp; hum.indexOf(params.sender) != -1) {<br />if(hum.length &gt; 1) {<br />a=Math.floor(Math.random()* hum.length);<br />b=hum[a];<br />params.replier.reply(&quot;&apos;&quot;+hum[a]+&quot;&apos;님부터 시작합니다!\\n1,2,3중 하나를 입력해주세요.&quot;);<br />st=true;<br />} else {<br />params.replier.reply(&quot;참가인원이 부족합니다.\\n현재 &quot;+hum.length+&quot;명&quot;);<br />}<br />}<br /><br />if(params.sender==b &amp;&amp; st==true &amp;&amp; !isNaN(params.msg)) {<br />if(params.msg.indexOf(&quot;.&quot;)==-1) {<br />if(Number(params.msg)&lt;=num+3 &amp;&amp; Number(params.msg)&gt;num) {<br />num+=(Number(params.msg)-num);<br />if(num&lt;31) {<br />hum.splice(hum.indexOf(b),1);<br />hum.push(b);<br />params.replier.reply(num+&quot;\\n&apos;&quot;+hum[0]+&quot;&apos;님은 &quot;+(num+1)+&quot;,&quot;+(num+2)+&quot;,&quot;+(num+3)+&quot; 중 하나를 입력해주세요.&quot;);<br />b=hum[0];<br />} else {<br />hum.splice(hum.indexOf(b),1);<br />params.replier.reply(&quot;이런, &apos;&quot;+b+&quot;&apos;님이 탈락하셨습니다! 게임 끝!\\n\\n[생존자]&quot;+hum.join(&quot;\\n&quot;));