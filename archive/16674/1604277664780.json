                    "content": "const admin = [\"K_S_A 대표 (볼펜)\"];//관리자 이름을 넣어주세요\n\nfunction response(room, msg, sender, isGroupChat, replier, imageDB, packageName){\n\nif (msg == \"/off\") {\nif(admin.indexOf(sender) != -1){\nApi.off();//이 스크립트 이름은 적으시면 안됩니다\nApi.on(\"스크립트이름\");//스크립트 이름 적어주기\nreplier.reply(\"전원이 꺼졌어요(off)\");\n}else{\nreplier.reply(sender+\"님은 관리자가 아닙니다!\");\n}\n}\n\nif (msg == \"/on\") {\nif(admin.indexOf(sender) != -1){\nApi.on();//Api.on(); 하면 다 켜져요\nreplier.reply(\"전원이 켜졌어요(on)\");\n}else{\nreplier.reply(sender+\"님은 관리자가 아닙니다!\");\n}\n}\n\nif(msg.indexOf(\"/관리자추가 \") == 0){\nif(admin.indexOf(sender) != -1){\nvar 관리자 = msg.substr(7);\nadmin.push(관리자);\nreplier.reply(sender+\"님이 \"+관리자+\"님을\\n관리자 목록에 추가시켰습니다!\");\n}else{\nreplier.reply(sender+\"님은 관리자가 아닌걸요...\");\n}\n}\n\nif(msg == \"/관리자목록\"){\nreplier.reply(\"관리자 리스트입니다\\n\\n\"+admin);\n}\n}",
