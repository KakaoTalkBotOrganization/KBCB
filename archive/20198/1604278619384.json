                {
                    "content": "function soulution(arr) {\r\n    let array = arr;\r\n    let result = [];\r\n    let last_arr = 0;\r\n    let count = 0;\r\n    let loop = true;\r\n    while(loop) {\r\n        if(last_arr == array[i]) {\r\n            count++;\r\n            break;\r\n        } \r\n        if(count <= 1 && last_arr != array[i]) {\r\n            result.push(count);\r\n            count = 0;\r\n            last_arr = array[i];\r\n            break;\r\n        } \r\n        if(array[i] == null) {\r\n            loop = false;\r\n            break;\r\n        } \r\n        if(last_arr != array[i]) {\r\n            last_arr = array[i];\r\n            break;\r\n        }\r\n    } if(result == null) {\r\n        result.push(-1);\r\n        return result;\r\n    } else {\r\n        return result;\r\n    }\r\n}",
