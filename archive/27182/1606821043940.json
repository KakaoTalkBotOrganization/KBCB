  <div class=\"se-module se-module-code se-fs-fs13\">\n                                <div class=\"se-code-source\">\n                                    <div class=\"__se_code_view language-javascript\">function getLastChat() {\n    let obj &#x3D; {};\n    let cursor &#x3D; db.rawQuery(&quot;select * from chat_logs&quot;, null);\n    cursor.moveToLast();\n    [&quot;_id&quot;, &quot;id&quot;, &quot;type&quot;, &quot;chat_id&quot;, &quot;user_id&quot;, &quot;message&quot;, &quot;attachment&quot;, &quot;created_at&quot;, &quot;deleted_at&quot;, &quot;client_message_id&quot;, &quot;prev_id&quot;, &quot;referer&quot;, &quot;supplement&quot;, &quot;v&quot;].forEach((e, i) &#x3D;&gt; {\n        obj[e] &#x3D; cursor.getString(i);\n        if (e &#x3D;&#x3D; &quot;v&quot;) {\n            obj.v &#x3D; JSON.parse(obj.v);\n        }\n    });\n    cursor.close();\n    obj.attachment &#x3D; decrypt(obj.user_id, obj.v.enc, obj.attachment);\n    obj.message &#x3D; decrypt(obj.user_id, obj.v.enc, obj.message);\n    return obj;\n}</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <script type=\"text/data\" class=\"__se_module_data\" data-module='{\"type\":\"v2_code\", \"id\" : \"SE-96ee1545-c9fd-42c6-9bef-8d6630ca14cd\"}'></script>\n                </div>                <div class=\"se-component se-text se-l-default\" id=\"SE-e8ff4681-6804-4673-bc88-f14bc5c4d97b\">\n                    <div class=\"se-component-content\">\n                        <div class=\"se-section se-section-text se-l-default\">\n                            <div class=\"se-module se-module-text\">\n\n                                    <!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"line-height:1.4615384615384615;\" id=\"SE-1a998e2d-5963-4266-b167-10242852f39a\"><span style=\"color:#000000;\" class=\"se-fs- se-ff-nanumsquare se-weight-unset  \" id=\"SE-86cc510a-a