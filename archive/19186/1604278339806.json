                    "content": "var 학습 = [];\nvar Lw = \"\\u200b\".repeat(500);\n\n\nvar AI = true;\nfunction response(room, msg, sender, isGroupChat, replier, imageDB, packageName) {\n\n\nvar 랜 = [Math.floor(Math.random()*학습.length)];\nvar 말 = [\"[Aoto] \"+학습[랜]];\n\n\nif(sender == \"관리자이름\"){\nif(msg == \"/자동학습초기화\"){\n학습 = [];\nreplier.reply(학습.length+\"개의 학습이 초기화 되었습니다\");\n}\n\nif(msg.startsWith(\"/자동학습제거 \")){\nssap = msg.substr(8);\nif(학습.indexOf(ssap) != -1){\n학습.splice(학습.indexOf(ssap), 1);\nreplier.reply(ssap+\"의 자동학습을 제거 했습니다\");\n}\nelse\nif(msg.startsWith(\"/자동학습제거 \")){\nreplier.reply(\"배우지 않은 학습입니다\");\n}\n}\nif(sender == \"관리자이름\"){\nif(msg == \"/자동학습 on\"){\nif(AI == false){\nAI = true;\nreplier.reply(\"자동학습이 켜졌어요!\");\n}\nelse\nif(sender == \"관리자이름\"){\nif(msg == \"/자동학습 on\"){\nreplier.reply(\"자동학습이 이미 켜져있어요!\");\n}\n}\n\nif(sender == \"관리자이름\"){\nif(msg == \"/자동학습 off\"){\nif(AI == true){\nAI = false;\nreplier.reply(\"자동학습이 꺼졌어요!\");\n}\nelse\nif(sender == \"관리자이름\"){\nif(msg == \"/자동학습 off\"){\nreplier.reply(\"자동학습이 이미 꺼져있어요!\");\n}\n}\n\nif(msg == \"/자동학습목록\"){\nreplier.reply(\"[자동학습목록]\"+Lw+\"\\n\\n\"+학습.join(\"\\n\"));\n}\n\nif(msg.startsWith(\"/\")){\nreturn;\n}\n\n\nb = (Math.floor(Math.random()*4));\nif(AI == true){\nif(msg&&b){\n학습.push(msg);\nreturn;\n}\n}\n\nif(AI == true){\nif(msg){\n학습.push(msg);\nreplier.reply(말);\n}\n}\n}",
