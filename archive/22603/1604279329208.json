class AddChatMessageAction implements Action\n{\n\tprivate static ListView listView;\n\n\t@Override\n\tpublic void init()\n\t{\n\t\tlistView &#x3D; DebugRoomTab.getComponent();\n\t}\n\n\tpublic static void execute(String message, boolean isbot)\n\t{\n\t\tHBox cell &#x3D; new HBox();\n\n\t\tLabel chat &#x3D; new Label();\n\t\tchat.setText(message);\n\t\tchat.setMaxWidth(220);\n\t\tchat.setWrapText(true);\n\n\t\tif (!isbot)\n\t\t{\n\t\t\tchat.getStyleClass().add(&quot;chat-sender&quot;);\n\t\t\tcell.setAlignment(Pos.TOP_RIGHT);\n\t\t}\n\t\telse\n\t\t{\n\t\t\tchat.getStyleClass().add(&quot;chat-bot&quot;);\n\t\t\tcell.setAlignment(Pos.TOP_LEFT);\n\t\t}\n\n\t\tcell.getChildren().add(chat);\n\t\tcell.setId(message);\n\n\t\tlistView.getItems().add(cell);\n\t\tlistView.scrollTo(cell);\n\t}\n\n\t@Override\n\tpublic String getName()\n\t{\n\t\treturn &quot;add.chat.message.action&quot;;\n\t}\n}</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <script type=\"text/data\" class=\"__se_module_data\" data-module='{\"type\":\"v2_code\", \"id\" : \"SE-80de8150-4148-4a72-9121-cd251395af4e\"}'></script>\n                </div>                <div class=\"se-component se-text se-l-default\" id=\"SE-1b94424f-fc6f-407f-b339-ac98a71f872f\">\n                    <div class=\"se-component-content\">\n                        <div class=\"se-section se-section-text se-l-default\">\n                            <div class=\"se-module se-module-text\">\n\n                                    <!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"line-height:2.0;\" id=\"SE-b39f8e03-f480-4a99-ae5c-2ee32a608729\"><span style=\"color:#000000;\" class=\"se-fs-fs13 se-ff-nanumsquare  se-style-unset \" id=\"SE-22dc1861-67e5-4a37-a1b6-df73e39d8065\">​</span></p><!-- } SE-TEXT --><!-- SE-TEXT { --><p class=\"se-text-paragraph se-text-paragraph-align- \" style=\"line