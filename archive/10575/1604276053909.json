            "contentHtml": "[[[CONTENT-ELEMENT-0]]]<br />[[[CONTENT-ELEMENT-1]]]<br /><p>function onCreate(savedInstanceState, activity) {<br />var layout = new android.widget.LinearLayout(activity);<br />layout.setOrientation(1);<br />layout.setGravity(android.view.Gravity.CENTER);<br /><br />var ed = new android.widget.EditText(activity);<br />ed.setHint(&quot;검색어&quot;);<br />layout.addView(ed);<br />var list = new android.widget.ListView(activity);<br />list.setLayoutParams(new android.widget.LinearLayout.LayoutParams(android.widget.LinearLayout.LayoutParams.MATCH_PARENT,android.widget.LinearLayout.LayoutParams.MATCH_PARENT));<br />var arr = new java.util.ArrayList(Object.getOwnPropertyNames(this));<br />var adapter = new android.widget.ArrayAdapter(activity, android.R.layout.simple_list_item_1, arr);<br /><br />list.setAdapter(adapter);<br />list.setTextFilterEnabled(true);<br />list.setOnItemClickListener(new android.widget.AdapterView.OnItemClickListener({<br />onItemClick: function(av, v, i, l) {<br />var str = list.getItemAtPosition(i).toString();<br />makeDialog(str, eval(str).toString(), activity);<br />}<br />}));<br /><br />var ad = list.getAdapter();<br /><br />ed.addTextChangedListener(new android.text.TextWatcher({<br /><br />beforeTextChanged: function(s,start,count,after) {<br />},<br /><br />onTextChanged:<br /><br />function(s,start,before,count) {<br /><br />try {<br />ad.getFilter().filter(s.toString());<br />} catch(err) { Log.e(&quot;에러:\\n &quot;+err+&quot;&quot;+err.lineNumber); }<br />} ,<br /><br />afterTextChanged: function(s) {<br /><br />try {<br /><br />if(ed.getText().length == 0) {<br />ad.getFilter().filter(null);<br />}<br />} catch(err) { Log.e(&quot;에러:\\n &quot;+err+&quot;&quot;+err.lineNumber); }<br />}<br />}));<br />layout.addView(list);<br />activity.setContentView(layout);<br />}<br /><br />function makeDialog(title,str,act) {<br />act.runOnUiThread(new java.lang.Runnable({<br />run: function(){<br />try{<br />var dialog = new android.app.AlertDialog.Builder(act);<br />dialog.setTitle(title);<br />dialog.setMessage(str);<br />dialog.setNegativeButton(&quot;확인&quot;, null);<br />dialog.show();<br />}<br />catch(e){<br />Log.e(e+&quot;, &quot;+e.lineNumber);<br />}<br />}<br />}));<br />}<br /><br />onCreate 부분을 지우고 이거로 다 빠꾸심 됩니다. 파일도 첨부합니다.<br /><br /></p>",
