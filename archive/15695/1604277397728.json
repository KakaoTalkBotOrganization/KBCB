            "contentHtml": "<p>function learn(word, answer) {<br />    FileStream.write(&quot;sdcard/learn/&quot; + word + &quot;/&quot;, answer);<br />    return word + &quot;라고 말하면 &quot; + answer + &quot;라고 대답합니다.&quot;;<br />}<br />function remove(word) {<br />    let a = FileStream.remove(&quot;sdcard/learn/&quot; + word);<br />    if (a == true) {<br />        return a;<br />    } else {<br />        return &quot;가르친적 없는 단어입니다.&quot;;<br />    }<br />}<br />function respond(word) {<br />    let a = FileStream.read(&quot;sdcard/learn/&quot; + word + &quot;/&quot;);<br />    return a;<br />}<br />function response(room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId) {<br />    let m = msg.split(&apos; &apos;);<br />    try {<br />        if (msg != &quot;.&quot;) {<br />            let a = FileStream.read(&quot;sdcard/learn/&quot; + msg + &quot;/&quot;);<br />            if (msg.indexOf(&quot;/가르치기 &quot;) == 0) {<br />                replier.reply(learn(m[1], m[2]));<br />            } else if (msg.indexOf(&quot;/학습제거&quot;) == 0) {<br />                replier.reply(remove(msg.substr(6)));<br />            } else if (a != null) {<br />                replier.reply(respond(msg));<br />            }<br />        }<br />    }  catch (e) {}<br />}</p>",
