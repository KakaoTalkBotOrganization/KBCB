            "contentHtml": "<p>const scriptName=&quot;App Builder.js&quot;;<br />//var 클립보드 = Api.getContext().getSystemService(android.content.Context.CLIPBOARD_SERVICE);<br /><br />ResourceDir=&quot;/sdcard/bin/&quot;<br />aapt=&quot;aapt&quot;<br />zipalign=&quot;zipalign&quot;<br />files=&quot;/data/data/com.xfl.kakaotalkbot/files/&quot;<br />copyAllFiles()<br />fixPermission()<br /><br />function response(room, msg, sender, isGroupChat, replier, ImageDB, packageName, threadId){<br />}<br /><br />function copy(src,dst){<br />try{<br />inStream = new java.io.FileInputStream(src);<br />outStream = new java.io.FileOutputStream(dst);<br />inChannel = inStream.getChannel();<br />outChannel = outStream.getChannel();<br />inChannel.transferTo(0, inChannel.size(), outChannel);<br />inStream.close();<br />outStream.close();<br />}catch(e){<br />Log.e(e)<br />}<br />}<br />build(&quot;/sdcard/testapp/&quot;)<br />function runExec(str,waitFor){<br />//클립보드.setText(str)<br />if(waitFor)<br />java.lang.Runtime.getRuntime().exec(str).waitFor()<br />else<br />java.lang.Runtime.getRuntime().exec(str)<br />}<br /><br />function copyAllFiles(){<br />copy(ResourceDir+aapt,&quot;/data/data/com.xfl.kakaotalkbot/files/aapt&quot;)<br />copy(ResourceDir+zipalign,&quot;/data/data/com.xfl.kakaotalkbot/files/zipalign&quot;)<br />//copy(ResourceDir+&quot;/android.jar&quot;,&quot;/data/data/com.xfl.kakaotalkbot/files/android.jar&quot;)<br />}<br /><br />function fixPermission(){<br />runExec(&quot;chmod a+rx /data/data/com.xfl.kakaotalkbot/files/aapt&quot;)<br />runExec(&quot;chmod a+rx /data/data/com.xfl.kakaotalkbot/files/zipalign&quot;)<br />}<br /><br />function build(buildDir){<br />runExec(files+&apos;aapt package -m -M &apos;+buildDir+&apos;AndroidManifest.xml -A &apos;+buildDir+&apos;assets -S &apos;+buildDir+&apos;res -J &apos;+buildDir+&apos;gen -I &apos;+ResourceDir+&apos;android.jar&apos;,true)<br />runExec(files+&apos;aapt package -f -M &apos;+buildDir+&apos;AndroidManifest.xml -S &apos;+buildDir+&apos;res -I &apos;+ResourceDir+&apos;android.jar -F &apos;+buildDir+&apos;MyProject.apk.unaligned&apos;,true)<br />//runExec(files+&apos;aapt add -f &apos;+buildDir+&apos;MyProject.apk.unaligned &apos;+buildDir+&apos;classes.dex&apos;)<br />//classes.dex 필요함<br />//classes.dex 만드려면 javac 필요함<br />//arm/aarch64 javac가 없ㄷ..<br />}</p>",
