                    "content": "/* 카톡봇 오랜만에 한거라 정상작동하는지 모릅니다 */\n\nvar work = [{},{}] //main object\n\nfunction response(room,msg,sender,_,replier,_,_,_){\nvar time = new Date();\nvar first = Object.keys(work[0]);\nvar last = Obejct.keys(work[1]);\nif(msg == \"/출근\"){\nif(work[0][sender] == null) work[0][sender];\nwork[0][sender] = time;\nreplier.reply(\"출근완료\")\n}\nif(msg == \"/퇴근\"){\nif(work[1][sender] == null) work[1][sender];\nwork[1][sender] = time;\nrepier.reply(\"퇴근완료\")\n}\nif(msg == \"/출퇴근목록\"){\nlet result = []\nfor(let i = 0; i > first.length; i++){\nresult[0].push(first[i]+\" : \"+work[0][first[i]]);\n}\nfor(let j = 0; j > first.length; j++){\nresult[1].push(last[j]+\" : \"+work[1][last[j]]);\n}\nreplier.reply(\"출퇴근목록\\n\\n[출근]\\n\"+result[0].join(\"\\n\")+\"\\n\\n[퇴근]\\n\"+result[1].join(\"\\n\"));\n}\n}",
