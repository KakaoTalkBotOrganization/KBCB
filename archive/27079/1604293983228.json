            "contentHtml": "<div class=\"se-viewer se-theme-default\" lang=\"ko-KR\">\n    <!-- SE_DOC_HEADER_START -->\n    <!--@CONTENTS_HEADER-->\n    <!-- SE_DOC_HEADER_END -->\n    <div class=\"se-main-container\">\n                <div class=\"se-component se-code se-l-code_black\" id=\"SE-79b59c0c-8e37-4f73-99b8-c6fe915849a9\">\n                    <div class=\"se-component-content\">\n                        <div class=\"se-section se-section-code se-l-code_black\">\n                            <div class=\"se-module se-module-code se-fs-fs13\">\n                                <div class=\"se-code-source\">\n                                    <div class=\"__se_code_view language-javascript\">let start;\nlet context &#x3D; [];\n\nfunction pingpong(query) {\n    const con &#x3D; org.jsoup.Jsoup.connect(&#x27;https://api-generation.pingpong.us/v0.1/generation/ko&#x27;)\n        .header(&#x27;Content-Type&#x27;, &#x27;application/json;charset&#x3D;UTF-8&#x27;)\n        .header(&#x27;Referer&#x27;, &#x27;https://pingpong.us/&#x27;)\n        .header(&#x27;cache-control&#x27;, &#x27;no-cache&#x27;)\n    context.push(query);\n    const payload &#x3D; {\n        context: context\n    };\n    con.requestBody(JSON.stringify(payload));\n    con.userAgent(&#x27;Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/86.0.4240.111 Safari/537.36&#x27;)\n    con.method(org.jsoup.Connection.Method.POST)\n    con.ignoreContentType(true)\n    const response &#x3D; con.execute();\n    if (response.statusCode() !&#x3D;&#x3D; 200) {\n        if (response.statusCode() &#x3D;&#x3D;&#x3D; 403)\n            throw new ReferenceError(&#x27;Invalid Request Payload.&#x27;);\n        else if (response.statusCode() &#x3D;&#x3D;&#x3D; 500)\n            throw new InternalError(&#x27;Daily Quota Exceeded.&#x27;);\n        else throw new JavaException(&#x27;Some Error Occured.&#x27;);\n    }\n    const { output } &#x3D; JSON.parse(response.body());\n    context.push(output);\n    return output;\n}\n\nfunction response(room, msg, sender, igc, replier) {\n    if (msg &#x3D;&#x3D;&#x3D; &#x27;on&#x27;)\n        replier.reply(start ? &#x27;이미 켜진 상태입니다.&#x27; : (start ^&#x3D; 1, &#x27;핑퐁이 활성화되었습니다.&#x27;));\n    else if (msg &#x3D;&#x3D;&#x3D; &#x27;off&#x27;)\n        replier.reply(start ? (start ^&#x3D; 1, &#x27;핑퐁이 비활성화되었습니다.&#x27;) : &#x27;현재 비활성화 상태입니다.&#x27;);\n    else if (msg[0] &#x3D;&#x3D;&#x3D; &#x27;.&#x27; &amp; start) {\n        try {\n            replier.reply(pingpong(msg.slice(1)));\n        } catch (e) {\n            replier.reply(e);\n        }\n    }\n}</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <script type=\"text/data\" class=\"__se_module_data\" data-module='{\"type\":\"v2_code\", \"id\" : \"SE-79b59c0c-8e37-4f73-99b8-c6fe915849a9\"}'></script>\n                </div>                <div class=\"se-component se-image se-l-default\" id=\"SE-502d7bc3-93da-4e55-8d47-9d7b1fb39188\">\n                    <div class=\"se-component-content se-component-content-normal\">\n                        <div class=\"se-section se-section-image se-l-default se-section-align-\" style=\"max-width:682px;\">\n\n                                <div class=\"se-module se-module-image\" style=\"\">\n                                    <a href=\"#\" class=\"se-module-image-link __se_image_link __se_link\" style=\"\" onclick=\"return false;\" data-linktype=\"img\" data-linkdata='{\"id\" : \"SE-502d7bc3-93da-4e55-8d47-9d7b1fb39188\", \"src\" : \"https://cafeptthumb-phinf.pstatic.net/MjAyMDExMDJfOTEg/MDAxNjA0Mjg2MjA3Nzg1.UtEWApQcnGViI_uFtrk4dfMV-D4G8V0BNcsFo3DNPS8g.yI2oJW5jjGqZ6zSnCva-SAM5C2RxPh8UIHk-t61qT3Ag.PNG/image.png\", \"linkUse\" : \"false\", \"link\" : \"\"}'>\n                                        <img src=\"https://mcafethumb-phinf.pstatic.net/MjAyMDExMDJfOTEg/MDAxNjA0Mjg2MjA3Nzg1.UtEWApQcnGViI_uFtrk4